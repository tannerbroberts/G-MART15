{
   "scripts": {
      "client:dev": "cd client && npm run dev",
      "client:build": "cd client && npm run build",
      "server:dev": "cd server && npm run dev",
      "server:watch": "cd server && npm run watch",
      "start": "concurrently \"npm run client:dev\" \"npm run server:watch\"",
      "build": "cd server && npm run build",
      "server:build": "cd server && npm run build",
      "serve": "cd server && node dist/index.js",
      "db:setup": "node ./scripts/setup-database.js",
      "MaggieSays": "npm install && cd client && npm install && cd ../server && npm install",
      "deploy:heroku": "git subtree push --prefix server heroku main",
      "deploy:heroku:force": "git push heroku `git subtree split --prefix server main`:main --force"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "devDependencies": {
      "@types/jsonwebtoken": "^9.0.9",
      "concurrently": "^9.1.2"
   },
   "dependencies": {
      "dotenv": "^16.5.0",
      "pg": "^8.14.1"
   }
}
